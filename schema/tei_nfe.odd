          <classSpec type="model" ident="model.placeNamePart" mode="delete"/>
          <classSpec type="model" ident="model.placeStateLike" mode="delete"/>
          <classSpec type="model" ident="model.specDescLike" mode="delete"/>
        </specGrp>
        <!-- 
